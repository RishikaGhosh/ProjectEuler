#include<bits/stdc++.h>
using namespace std;
int main()
{
	#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
     #endif 
    int result = 0;
     for (int i = 1; i < 1000; i++)
      {
    if (((i % 3) == 0) || ((i % 5) == 0))
     {
        result += i;
      }
   }
   cout<<result;
}
